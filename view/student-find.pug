extend base

block content 
    .student-display
        each student in students 
            .student-pallet
                ul
                    li= `Session: ${student.year}`
                    li= `Class: ${student.class}`
                    li= `Roll No.: ${student.roll_no}`
                    li= `Student Name: ${student.name}`
                    li= `Father Name: ${student.father_name}`
                    a(href=`/studentInfo/get-one/${student._id}`)
                        button.get-student-info Get student